#include <stdio.h>
typedef struct
{
    int x;
    int y;
}points;

void print_points(points p[]);
int main()
{
    points p[5];
    for (char i=0;i<9;i++)
    {
        scanf("%d%d",&p[i].x,&p[i].y);
    }
    print_points(p);

    return 0;
}
void print_points(points p[])
{
 for(char i=0;i<5;i++)
 {
  printf("P%d of x co-ordinate %d & y co-ordinate is %d\n",i,p[i].x,p[i].y);  
 }
}
